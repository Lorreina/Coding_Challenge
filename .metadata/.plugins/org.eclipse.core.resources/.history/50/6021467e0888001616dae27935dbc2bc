 */
public class Snychronization_and_Locks {
	public class MyCalss extends Thread {
		private String name;
		private MyObject myObj;
		
		public MyClass(My Obj, String n) {
			this.name = n;
			this.myObj = Obj;
		}
		
		public void run() {
			myObj.foo(name);
		}
	}
	
	public class MyObject {
		public synchronized void foo(String name) {
			
		}
	}
}
