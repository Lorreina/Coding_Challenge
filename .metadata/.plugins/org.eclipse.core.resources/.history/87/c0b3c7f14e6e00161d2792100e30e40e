
/**
 * LeetCode
 * 108. Convert Sorted Array to Binary Search Tree
 * @author lorreina
 *
 */
public class Convert_Sorted_Array_to_Binary_Search_Tree {
	
	
	
	
	/**
	 * Definition for a binary tree node.
	 * public class TreeNode {
	 *     int val;
	 *     TreeNode left;
	 *     TreeNode right;
	 *     TreeNode(int x) { val = x; }
	 * }
	 */
	public class TreeNode {
		int val;
		TreeNode left;
		TreeNode right;
		TreeNode(int x) { val = x; }
	}
}
