/**
 * LeetCode
 * 36. Valid Sudoku
 * @author lorreina
 *
 */
public class Valid_Sudoku {
	public boolean isValidSudoku(char[][] board) {
        if (board == null || board.length != 9 || board[0].length != 9) {
            return false;
        }
        HashSet<Character> set = new HashSet<Character> ();
        for (int i = 0; i < board.length; i++) {
            set.clear();
            for (int j = 0; j < board[0].length; j++) {
                if (board[i][i] == '.') {
                    continue;
                }
                if (set.contains(board[i][j])) {
                    return false;
                }
                set.add(board[i][j]);
            }
        }
        
        for (int k = 0; k < 9; k += 3) {
            for (int p = 0; p < 9; p += 3) {
                for (int i = k; i < k + 3; i++) {
                    set.clear();
                    for (int j = p; j < p + 3; j++) {
                        if (board[i][j] == '.') {
                            continue;
                        }
                        if (set.contains(board[i][j])) {
                            return false;
                        }
                        set.add(board[i][j]);
                    }
                }
            }
        }
        
        return true;
    }
	
	public static void main(String[] args) {
		int[][] matrix = new int[9][9];
		matrix[0][1] = 5;
		matrix[0][8] = 6;
		matrix[1][4] = 1;
		matrix[1][5] = 4'
				matrix[]
	}
}
