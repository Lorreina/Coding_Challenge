
public class Split_Odd_Even {
	public void splitArray(int[] nums) {
		if (nums == null || nums.length < 2) {
			return;
		}
		
		int left = 0, right = nums.length - 1;
		
		while (left < right) {
			
		}
		
		
	}
	
	private void swap(int[] nums, int left, int right) {
		int tmp = nums[left];
		nums[left] = nums[right];
		nums[right] = tmp;
	}
}
